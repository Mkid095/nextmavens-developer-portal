{
  "name": "nextmavens-cli",
  "version": "1.0.0",
  "description": "CLI tool for NextMavens platform - Manage projects, databases, functions, and secrets from the command line",
  "main": "dist/index.js",
  "bin": {
    "nextmavens": "./dist/index.js"
  },
  "scripts": {
    "build": "node build.js",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build",
    "typecheck": "cd .. && tsc --noEmit -p cli/tsconfig.json",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cli",
    "nextmavens",
    "database",
    "serverless",
    "edge-functions",
    "developer-tools",
    "platform-cli"
  ],
  "license": "MIT",
  "author": "NextMavens",
  "homepage": "https://nextmavens.com/docs",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextmavens/nextmavens-developer-portal.git",
    "directory": "cli"
  },
  "bugs": {
    "url": "https://github.com/nextmavens/nextmavens-developer-portal/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
